  "scripts": {
    "build": "rimraf dist && nx run-many --target build --all --parallel",
    "build:deps": "rimraf dist && nx run-many --target build-deps --all --parallel",
    "build:core": "npx nx build core",
    "build:main": "npx nx build main",
    "build:service-a": "npx nx build serviceA",
    "build:service-b": "npx nx build serviceB",
    "start": "nx run-many --target serve --all --parallel",
    "start:main": "nx run main:serve",
    "start:service-a": "nx run service-a:serve",
    "start:service-b": "nx run service-b:serve",
    "typecheck": "nx run-many --target typecheck --all --parallel",
    "lint": "nx run-many --target lint --all --parallel",
    "format": "nx run-many --target format --all --parallel",
    "format:check": "nx run-many --target format:check --all --parallel",
    "test:watch": "nx run-many --target test --all --parallel --watch",
    "test:ci": "nx run-many --target test --all --parallel --ci",
    "test:debug": "nx run-many --target test --all --parallel --debug",
    "test:debug:watch": "nx run-many --target test --all --parallel",
    "migration:run": "nx run core:migrate",
    "migration:generate": "typeorm migration:generate -d libs/core/src/migrations/data-source.ts libs/core/src/migrations/generated"
  },
  "engines": {
    "node": ">=22.0.0 <24.0.0"
  },
